 <!-- select查询语句 Start-->
<!-- huchao -->
<!-- 根据地区表组查询表信息 -->
   <select id="querySubareaByCond" resultType="java.util.Map" parameterType="java.util.Map">
   	select <include refid="Base_Subarea"/> from subarea a
   	where 1=1  
		<if test="subareaId != null and subareaId != '' " >
		    and  a.subarea_id = #{subareaId}   
	     </if>
		<if test="showId != null and showId != '' " >
		    and  a.show_id = #{showId}   
	     </if>
		<if test="province != null and province != '' " >
		    and  a.province = #{province}   
	     </if>
		<if test="city != null and city != '' " >
		    and  a.city = #{city}   
	     </if>
		<if test="district != null and district != '' " >
		    and  a.district = #{district}   
	     </if>
		<if test="addresskey != null and addresskey != '' " >
		    and  a.addresskey = #{addresskey}   
	     </if>
		<if test="startnum != null and startnum != '' " >
		    and  a.startnum = #{startnum}   
	     </if>
		<if test="endnum != null and endnum != '' " >
		    and  a.endnum = #{endnum}   
	     </if>
		<if test="single != null and single != '' " >
		    and  a.single = #{single}   
	     </if>
		<if test="position != null and position != '' " >
		    and  a.position = #{position}   
	     </if>
		<if test="regionId != null and regionId != '' " >
		    and  a.region_id = #{regionId}   
	     </if>
      order by a.id asc
	<if test="pageFlag == 'true' ">
	  	limit #{startRow} ,#{pageSize} 
	</if> 
   </select>
<!-- huchao -->
 
 
