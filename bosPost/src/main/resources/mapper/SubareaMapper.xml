  <sql id="Property_Column_Map" >
  <!-- 表字段 -->
       
  </sql>
  <sql id="Base_Subarea">
  a.id  id,a.subarea_id  subareaId,a.show_id  showId,a.province  province,a.city  city,a.district  district,a.addresskey  addresskey,a.startnum  startnum,a.endnum  endnum,a.single  single,a.position  position,a.region_id  regionId
